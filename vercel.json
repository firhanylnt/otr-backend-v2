{
  "version": 2,
  "framework": null,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "public",
  "rewrites": [{ "source": "/(.*)", "destination": "/api?__path=$1" }],
  "functions": { "api/index.js": { "maxDuration": 30, "memory": 1024 } },
  "env": { "NODE_ENV": "production" }
}
